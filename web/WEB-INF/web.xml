<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>gpac</servlet-name>
        <servlet-class>gpac</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginservlet</servlet-name>
        <servlet-class>loginservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginvit</servlet-name>
        <servlet-class>loginvit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>myprofile</servlet-name>
        <servlet-class>myprofile.myprofile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>facloginservlet</servlet-name>
        <servlet-class>facloginservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gpac</servlet-name>
        <url-pattern>/gpac</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginservlet</servlet-name>
        <url-pattern>/loginservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginvit</servlet-name>
        <url-pattern>/loginvit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>myprofile</servlet-name>
        <url-pattern>/myprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>facloginservlet</servlet-name>
        <url-pattern>/facloginservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
